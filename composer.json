{
    "name": "jalallinux/laravel-tomanpay",
    "description": "Laravel Tomanpay integeration SDK",
    "keywords": [
        "HiDevs",
        "JalalLinuX",
        "laravel",
        "laravel-tomanpay"
    ],
    "homepage": "https://github.com/jalallinux/laravel-tomanpay",
    "license": "MIT",
    "authors": [
        {
            "name": "JalalLinuX",
            "email": "smjjalalzadeh93@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "illuminate/http": "^9.0|^10.0",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/contracts": "^9.0|^10.0"
    },
    "require-dev": {
        "laravel/pint": "*",
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "JalalLinuX\\Toman\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JalalLinuX\\Toman\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "php-cs-fixer fix --allow-risky=yes",
        "pint": "vendor/bin/pint --config pint.json"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
